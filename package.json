{
  "name": "constanta",
  "version": "1.0.0",
  "private": true,
  "description": "Верстка лендинга constanta",
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/scss/**/*.scss\" --syntax scss",
    "eslint": "eslint --ext .js source/js",
    "test": "npm run editorconfig && npm run stylelint && npm run eslint",
    "build": "gulp build",
    "start": "gulp start"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/sass/**/*.{sass,scss}"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BeHustle/constanta.git"
  },
  "keywords": [
    "html",
    "css",
    "layout",
    "responsive",
    "adaptive"
  ],
  "author": "Kleva Vladislav <i.vlad.kleva@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BeHustle/constanta/issues"
  },
  "engines": {
    "node": "12"
  },
  "homepage": "https://github.com/BeHustle/constanta#readme",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "10.0.1",
    "browser-sync": "2.26.13",
    "del": "6.0.0",
    "eslint": "7.13.0",
    "eslint-config-htmlacademy": "0.6.0",
    "gulp": "4.0.2",
    "gulp-concat": "2.6.1",
    "gulp-csso": "4.0.1",
    "gulp-minify": "3.1.0",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.0",
    "gulp-rename": "2.0.0",
    "gulp-sass": "4.1.0",
    "gulp-sourcemaps": "2.6.5",
    "picturefill": "3.0.3",
    "postcss": "8.2.1",
    "stylelint": "13.7.2",
    "stylelint-config-htmlacademy": "0.1.4",
    "svg4everybody": "2.1.9"
  },
  "dependencies": {
    "normalize.css": "8.0.1"
  }
}
